<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smhrd.database.CommentsMapper">

	<insert id="insertComments" parameterType="com.smhrd.domain.Comments">
		insert into COMMENTS (comments_num, comments, mem_num) values(COMMENTS_SEQ.nextval,#{comments},#{mem_num})
	</insert>
	
</mapper>